{
	"batch_size": 64,
	"dropout_keep_rate": 0.7,
	"learning_rate": 0.0001,
	"gradient_clip": 1,
	"epochs": 500,
	"patience": 10,
    "model": "standard",
    "input_noise": 0.001,
    "seq_length": 80,
    "dataset_type": "standard",
	"model_params": {
		"state_size": 20,
		"output_hidden_units": [32, 32],
		"output_hidden_activation": "relu",
        "output_type": "multi_classification",
	    "pool_mode": "attention",
        "mlp_hidden_units": [32, 32],
        "mlp_activation": "relu",
		"model_type": "nbow",
        "loss_mode": "cross-entropy",
        "normalize_inputs": true,
        "use_conv_embedding": false,
        "embedding_layer_params": {
            "filter_widths": [3, 3, 3],
            "filter_heights": [3, 3, 3],
            "conv_activation": "relu",
            "dense_activation": "relu",
            "filter_channels": [1, 1, 1],
            "filter_strides": [1, 2, 2]
        }
	}
}
