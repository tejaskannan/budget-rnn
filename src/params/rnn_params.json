{
	"batch_size": 64,
	"dropout_keep_rate": 0.7,
	"learning_rate": 0.0001,
	"gradient_clip": 1,
	"epochs": 10,
	"patience": 25,
    "model": "standard",
    "dataset_type": "standard",
    "input_noise": 0.001,
    "seq_length": 80,
	"model_params": {
		"state_size": 20,
		"output_hidden_units": [32, 32],
		"output_hidden_activation": "leaky_relu",
        "output_type": "multi_classification",
	    "pool_mode": "weighted_average",
        "rnn_cell_type": "gru",
        "rnn_layers": 2,
        "rnn_activation": "linear_tanh",
		"model_type": "rnn",
        "loss_mode": "cross-entropy",
        "normalize_inputs": true,
        "use_conv_embedding": false,
        "embedding_layer_params": {
            "filter_widths": [3, 3, 3],
            "filter_heights": [3, 3, 3],
            "conv_activation": "leaky_relu",
            "dense_activation": "leaky_relu",
            "filter_channels": [1, 1, 1],
            "filter_strides": [1, 2, 2]
        }
	}
}
