{
	"batch_size": 64,
	"dropout_keep_rate": 0.7,
	"learning_rate": 0.0001,
	"gradient_clip": 1,
	"epochs": 500,
	"patience": 25,
    "model": "adaptive",
    "dataset_type": "standard",
    "input_noise": 0.001,
    "seq_length": 20,
	"model_params": {
		"state_size": 20,
		"sample_frac": 0.2,
		"rnn_cell_type": "gru",
		"rnn_activation": "tanh",
		"rnn_layers": 1,
		"output_hidden_units": [32, 32],
		"output_hidden_activation": "relu",
        "output_type": "multi_classification",
		"layer_weight": 1.0,
		"combine_element_wise": true,
		"pool_mode": "final_state",
		"model_type": "sample",
		"share_cell_weights": true,
        "share_output_weights": true,
        "share_rnn_weights": true,
		"loss_weights": 1.0,
        "link_levels": false,
        "loss_mode": "cross-entropy",
        "fusion_mode": "gate",
        "normalize_inputs": true,
        "regularization_name": "l1",
        "regularization_scale": 0.001,
	"compression_fraction": 0.125,
        "use_conv_embedding": false,
        "embedding_layer_params": {
            "filter_widths": [3, 3, 3],
            "filter_heights": [3, 3, 3],
            "conv_activation": "relu",
            "dense_activation": "relu",
            "filter_channels": [1, 1, 1],
            "filter_strides": [1, 2, 2]
        }
	}
}
