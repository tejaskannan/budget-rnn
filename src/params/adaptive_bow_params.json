{
	"batch_size": 64,
	"dropout_keep_rate": 0.9,
	"learning_rate": 0.0001,
	"gradient_clip": 1,
	"epochs": 5,
	"patience": 25,
    "model": "adaptive",
    "dataset_type": "standard",
    "input_noise": 0.0001,
    "seq_length": 12,
	"model_params": {
		"state_size": 16,
		"sample_frac": 0.25,
		"output_hidden_units": [16],
		"output_hidden_activation": "tanh",
        "transform_units": [32, 32],
        "transform_activation": "tanh",
        "attn_activation": "tanh",
        "output_type": "multi_classification",
		"model_type": "bow",
		"share_transform_weights": true,
        "share_output_weights": true,
		"loss_weights": [1.0, 1.0, 1.0, 1.0],
        "loss_mode": "cross-entropy",
        "normalize_inputs": true,
        "use_conv_embedding": false,
        "embedding_layer_params": {
            "filter_widths": [3, 3, 3],
            "filter_heights": [3, 3, 3],
            "conv_activation": "relu",
            "dense_activation": "tanh",
            "filter_channels": [1, 1, 1],
            "filter_strides": [1, 2, 2]
        }
	}
}
