{
	"batch_size": 8,
	"dropout_keep_rate": 0.9,
	"learning_rate": 0.0001,
	"gradient_clip": 1,
	"epochs": 250,
	"patience":20,
    "model": "adaptive",
    "dataset_type": "standard",
    "input_noise": 0.001,
    "seq_length": 20,
	"model_params": {
		"state_size": 10,
		"sample_frac": 0.1,
		"output_hidden_units": [12, 12],
		"output_hidden_activation": "leaky_relu",
        "transform_units": [12, 12],
        "transform_activation": "leaky_relu",
        "embedding_activation": "leaky_relu",
        "attn_activation": "sigmoid",
        "output_type": "multi_classification",
		"model_type": "adaptive_nbow",
		"share_transform_weights": true,
        "share_output_weights": true,
        "share_embedding_weights": true,
		"loss_weights": true,
        "enforce_level_penalty": false,
        "loss_mode": "cross-entropy",
        "normalize_inputs": true,
        "use_stop_layer": true,
        "stop_loss_weight": 0.1,
        "stop_output_hidden_units": [4],
        "stop_output_activation": "leaky_relu"
	}
}
