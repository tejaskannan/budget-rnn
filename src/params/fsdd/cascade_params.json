{
	"batch_size": 8,
	"dropout_keep_rate": 0.9,
	"learning_rate": 0.0001,
    "learning_rate_decay": 0.999,
	"gradient_clip": 1,
	"epochs": 250,
	"patience": 20,
    "model": "adaptive",
    "dataset_type": "standard",
    "input_noise": 0.001,
    "seq_length": 30,
	"model_params": {
		"state_size": 10,
		"sample_frac": 0.1,
		"rnn_cell_type": "gru",
		"rnn_activation": "tanh",
		"rnn_layers": 1,
		"output_hidden_units": [12],
		"output_hidden_activation": "leaky_relu",
        "output_type": "multi_classification",
        "embedding_activation": "leaky_relu",
		"pool_mode": "final_state",
		"model_type": "cascade",
		"share_cell_weights": true,
        "share_output_weights": true,
        "share_rnn_weights": true,
        "share_embedding_weights": true,
		"loss_weights": true,
        "loss_mode": "cross-entropy",
        "enforce_level_penalty": false,
        "fusion_mode": "identity",
        "normalize_inputs": true,
	"use_stop_layer": true,
	"stop_loss_weight": 0.1,
	"stop_output_activation": "leaky_relu",
	"stop_output_hidden_units": [4]
	}
}
