{
	"batch_size": 32,
	"dropout_keep_rate": 0.7,
	"learning_rate": 0.0001,
	"gradient_clip": 1,
	"epochs": 250,
	"patience": 10,
    "model": "adaptive",
    "dataset_type": "standard",
    "input_noise": 0.001,
    "batch_noise": 0.0,
    "seq_length": 80,
	"model_params": {
		"state_size": 20,
		"sample_frac": 0.1,
		"output_hidden_units": [24, 24],
		"output_hidden_activation": "relu",
        "transform_units": [24, 24],
        "transform_activation": "relu",
        "embedding_activation": "relu",
        "attn_activation": "sigmoid",
        "output_type": "multi_classification",
		"model_type": "bow",
		"share_transform_weights": true,
        "share_output_weights": true,
		"loss_weights": true,
        "loss_mode": "cross-entropy",
        "normalize_inputs": true,
        "compression_fraction": 0.2,
        "enforce_level_penalty": false
	}
}
