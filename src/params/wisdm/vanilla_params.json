{
	"batch_size": 32,
	"dropout_keep_rate": 0.7,
	"learning_rate": 0.0001,
	"gradient_clip": 1,
	"epochs": 250,
	"patience": 10,
    "model": "adaptive",
    "dataset_type": "standard",
