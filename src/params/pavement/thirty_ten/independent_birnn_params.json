{
	"batch_size": 8,
	"dropout_keep_rate": 0.9,
	"learning_rate": 0.0001,
	"gradient_clip": 1,
	"epochs": 250,
	"patience": 20,
    "model": "adaptive",
    "dataset_type": "standard",
    "input_noise": 0.001,
    "seq_length": 30,
	"model_params": {
		"state_size": 16,
		"sample_frac": 0.1,
		"rnn_cell_type": "gru",
		"rnn_activation": "tanh",
		"rnn_layers": 1,
		"output_hidden_units": [24, 24],
		"output_hidden_activation": "leaky_relu",
        "output_type": "multi_classification",
        "embedding_activation": "leaky_relu",
		"pool_mode": "final_state",
		"model_type": "independent_birnn",
		"share_cell_weights": false,
        "share_rnn_weights": false,
        "share_output_weights": false,
        "share_embedding_weights": false,
		"loss_weights": false,
        "loss_mode": "cross-entropy",
        "enforce_level_penalty": false,
        "fusion_mode": "identity",
        "normalize_inputs": true,
        "compression_fraction": 0.15
	}
}
